
      <table class="table table-dark">
        <thead>
          <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Description</th>
            <th scope="col">Price</th>
            <th scope="col">Image</th>
            <th scope="col">Actions</th>
          </tr>
        </thead>
        <tbody>
          <!-- for every row in we will use for each loop to render array of products data -->
           <!-- basically using ejs we are including JS code inside html -->
          <% products.forEach((product) => { %>
          <tr>
            <!-- Render product data by % and equal -->
            <th><%= product.id %></th>
            <td><%= product.name %></td>
            <td><%= product.desc %></td>
            <td scope="col"><%= product.price %></td>
            <td>
              <!-- <img
                src="https://m.media-amazon.com/images/I/51-nXsSRfZL._SX328_BO1,204,203,200_.jpg"
              /> -->
              <img
                src="<%= product.imageUrl %>"
              />
            </td>
            <td>
              <a href="/update-product/<%= product.id %>" class="btn btn-warning">Update</a>
            </td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    
